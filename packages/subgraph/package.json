{
  "name": "@omnibridge-nft/subgraph",
  "version": "1.0.0",
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.21.1",
    "@graphprotocol/graph-ts": "^0.20.1"
  },
  "license": "MIT",
  "scripts": {
    "auth": "graph auth https://api.thegraph.com/deploy/ $GRAPH_ACCESS_TOKEN",
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "prepare-mainnet-xdai": "mustache config/mainnet-xdai.json subgraph.template.yaml > subgraph.yaml",
    "deploy-mainnet-xdai": "graph deploy dan13ram/mainnet-xdai-omnibridge-nft --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-xdai-mainnet": "mustache config/xdai-mainnet.json subgraph.template.yaml > subgraph.yaml",
    "deploy-xdai-mainnet": "graph deploy dan13ram/xdai-mainnet-omnibridge-nft --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-rinkeby-xdai": "mustache config/rinkeby-xdai.json subgraph.template.yaml > subgraph.yaml",
    "deploy-rinkeby-xdai": "graph deploy dan13ram/rinkeby-xdai-omnibridge-nft --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-xdai-rinkeby": "mustache config/xdai-rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "deploy-xdai-rinkeby": "graph deploy dan13ram/xdai-rinkeby-omnibridge-nft --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-kovan-sokol": "mustache config/kovan-sokol.json subgraph.template.yaml > subgraph.yaml",
    "deploy-kovan-sokol": "graph deploy dan13ram/kovan-sokol-omnibridge-nft --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-sokol-kovan": "mustache config/sokol-kovan.json subgraph.template.yaml > subgraph.yaml",
    "deploy-sokol-kovan": "graph deploy dan13ram/sokol-kovan-omnibridge-nft --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-gusandbox-goerli": "mustache config/gusandbox-goerli-dev.json subgraph.template.yaml > subgraph.yaml",
    "deploy-gusandbox-goerli": "graph deploy cuonghx-gutech/gusandbox-goerli-omnibridge-nft-dev --debug --ipfs https://api.thegraph.com/ipfs/ --node http://34.85.33.121:8020",
    "prepare-goerli-gusandbox-dev": "mustache config/goerli-gusandbox-dev.json subgraph.template.yaml > subgraph.yaml",
    "deploy-goerli-gusandbox-dev": "graph deploy cuonghx1108/goerli-gusandbox-nft-dev --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-gusandbox-goerli-test": "mustache config/gusandbox-goerli-test.json subgraph.template.yaml > subgraph.yaml",
    "create-gusandbox-goerli-test": "graph create cuonghx/gusandbox-goerli-omnibridge-nft-test-1 --node http://34.85.33.121:8020",
    "deploy-gusandbox-goerli-test": "graph deploy cuonghx/gusandbox-goerli-omnibridge-nft-test-1 --ipfs https://api.thegraph.com/ipfs/ --node http://34.85.33.121:8020",
    "prepare-goerli-gusandbox-test": "mustache config/goerli-gusandbox-test.json subgraph.template.yaml > subgraph.yaml",
    "deploy-goerli-gusandbox-test": "graph deploy --product hosted-service cuonghx1108/goerli-gu-nft-test-1",
    "prepare-goerli-gusandbox-stage": "mustache config/goerli-gusandbox-stage.json subgraph.template.yaml > subgraph.yaml",
    "deploy-goerli-gusandbox-stage": "graph deploy kumikotamura/gu-nft-subgraph-staging --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare-gusandbox-goerli-stage": "mustache config/gusandbox-goerli-stage.json subgraph.template.yaml > subgraph.yaml",
    "create-gusandbox-goerli-stage": "graph create kumikotamura/gu-nft-subgraph-staging --node http://34.85.33.121:8020",
    "deploy-gusandbox-goerli-stage": "graph deploy kumikotamura/gu-nft-subgraph-staging --ipfs http://34.85.33.121:5001 --node http://34.85.33.121:8020",
    "prepare-joc": "mustache config/joc-ethereum.json subgraph.template.yaml > subgraph.yaml",
    "create-joc": "graph create gulaps/gubridge-joc-ethereum --node https://graphnode-admin.mainnet.japanopenchain.org",
    "deploy-joc": "graph deploy gulaps/gubridge-joc-ethereum --node https://graphnode-admin.mainnet.japanopenchain.org --ipfs https://graphnode-ipfs.mainnet.japanopenchain.org"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}
